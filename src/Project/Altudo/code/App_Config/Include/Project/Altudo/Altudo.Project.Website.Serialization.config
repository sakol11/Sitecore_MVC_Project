<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
	<sitecore>
		<unicorn>
			<configurations>
				<configuration name="Project.Altudo" dependencies="Foundation.Serialization" description="All project level configs like, page templates, layouts, base page templates etc., are serialized here">
					<targetDataStore physicalRootPath="C:\SitecoreProject\src\$(layer)\$(module)\serialization" />
						<predicate>
						<include name="Templates" database="master" path="/sitecore/templates/Project/VisitUs" />
						<include name="Workflow" database="master" path="/sitecore/system/Workflows/VisitUsWorkFlow" />
						<include name="Languages" database="master" path="/sitecore/system/Languages" />
							<include name="ChunkDisplay Name" database="core" path="/sitecore/content/Applications/Content Editor/Ribbons/Chunks/ItemDisplayname" />
							<include name="ChunkGetChildItem Count" database="core" path="/sitecore/content/Applications/Content Editor/Ribbons/Chunks/GetChildItem Count" />
							<include name="StripHouseKeeping" database="core" path="/sitecore/content/Applications/Content Editor/Ribbons/Strips/HouseKeeping" />
							<include name="RibbonHouseKeeping" database="core" path="/sitecore/content/Applications/Content Editor/Ribbons/Ribbons/Default/Housekeeping" />
					</predicate>
					<dataProviderConfiguration enableTransparentSync="false" />
					<syncConfiguration updateLinkDatabase="false" updateSearchIndex="false" />
				</configuration>
			</configurations>
		</unicorn>
	</sitecore>
</configuration>